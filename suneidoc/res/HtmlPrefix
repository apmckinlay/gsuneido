</style>

<style type="text/css" media="screen">
.ss { height: 150px; float: right; }
.showhide { display: none; }
</style>

<style type="text/css" media="print">
body { font-family: Georgia,'Times New Roman',Times,serif; }
h1,h2,h3,h4,h5,h6 { font-family: Verdana,Arial,Helvetica,sans-serif; }
h1,h2 { page-break-before: always; }
h1,h2,h3,h4,h5,h6 { page-break-after: avoid; }
.ss { width: 6in; margin-top: 6px; }
.showhide { display: inline; visibility: visible; }
</style>

<script language="JavaScript">
function screenshot(index)
    {
    if (document.getElementById("ss" + index).style.height == "auto")
        {
        document.getElementById("ss" + index).style.float = "right"
        document.getElementById("ss" + index).style.height = "150px"
        }
    else
        {
        document.getElementById("ss" + index).style.float = "none"
        document.getElementById("ss" + index).style.height ="auto"
        }
    }
function showhide(item)
    {
    element = document.getElementById(item)
    if (element.style.visibility == "visible")
        {
        element.style.visibility="hidden";
        element.style.display="none";
        }
    else
        {
        element.style.visibility="visible";
        element.style.display="inline";
        }
    }
</script>
